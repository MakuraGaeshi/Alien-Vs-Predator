		<allowHumanBios>false</allowHumanBios>
		<maleGenderProbability>0.5</maleGenderProbability>
	  </li>
	</modExtensions>
  </PawnKindDef>

  <PawnKindDef ParentName="BaseYautjaPawnKind">
