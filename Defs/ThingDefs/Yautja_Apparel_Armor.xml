	<!-- ========================= Yautja - Hunter Light Armour Chestplate ========================== -->
	<ThingDef ParentName="RRYYautjaArmourBase">
		<defName>RRY_Apparel_HunterLightArmourChestplate</defName>
		<label>Yautja Light Chestplate</label>
		<!-- <description>.</description> -->
		<graphicData>
			<texPath>Things/Apparel/Yautja_Light_Armour/Yautja_LightArmour</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>2500</MarketValue>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToMake>60000</WorkToMake>
			<ArmorRating_Blunt>0.55</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.35</ArmorRating_Sharp>
			<ArmorRating_Heat>0.35</ArmorRating_Heat>
			<Insulation_Heat>7</Insulation_Heat>
			<Insulation_Cold>7</Insulation_Cold>
			<Mass>3</Mass>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>+0.03</MoveSpeed>
			<WorkSpeedGlobal>-0.05</WorkSpeedGlobal>
		<!--
			<ShootingAccuracyPawn>0.25</ShootingAccuracyPawn>
			<PsychicSensitivity>-0.10</PsychicSensitivity>
			<ToxicSensitivity>-0.5</ToxicSensitivity>
			-->
		</equippedStatOffsets>
		<costList>
			<Steel>80</Steel>
			<Plasteel>40</Plasteel>
			<ComponentIndustrial>6</ComponentIndustrial>
		</costList>
		<recipeMaker>
			
		</recipeMaker>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/Yautja_Light_Armour/Yautja_LightArmour</wornGraphicPath>
			<layers>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<tags>
				<li>YautjaLightChestplate</li>
				<li>YautjaLightArmour</li>
				<li>Yautja</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<thingCategories>
			<li>RRYArmour</li>
		</thingCategories>
		<tradeTags>
			<li>Apparel</li>
		</tradeTags>
	</ThingDef>

