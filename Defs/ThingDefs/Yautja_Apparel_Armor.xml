	<!-- ========================= Yautja - Hunter Light Armour Chestplate ========================== -->
	<ThingDef ParentName="RRY_Yautja_ArmourBase">
		<defName>RRY_Apparel_HunterLightArmourChestplate</defName>
		<label>Yautja Light Chestplate</label>
		<description>A durable and lightweight chesplate. While it doesn't protect as effectively as the heavier sets, this piece of armour was made with freedom of movement in mind, so can help the best runner be a little faster.</description>
		<graphicData>
			<texPath>Things/Apparel/Yautja_Light_Armour/Yautja_LightArmour</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>1500</MarketValue>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToMake>40000</WorkToMake>
			<ArmorRating_Blunt>0.45</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.30</ArmorRating_Sharp>
			<ArmorRating_Heat>0.30</ArmorRating_Heat>
			<Insulation_Heat>6</Insulation_Heat>
			<Insulation_Cold>6</Insulation_Cold>
			<Mass>3</Mass>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>+0.1</MoveSpeed>
			<WorkSpeedGlobal>-0.05</WorkSpeedGlobal>
		<!--
			<ShootingAccuracyPawn>0.25</ShootingAccuracyPawn>
			<PsychicSensitivity>-0.10</PsychicSensitivity>
			<ToxicSensitivity>-0.5</ToxicSensitivity>
			-->
		</equippedStatOffsets>
		<costList>
			<Steel>80</Steel>
			<Plasteel>40</Plasteel>
			<ComponentIndustrial>6</ComponentIndustrial>
		</costList>
		<recipeMaker>
			<researchPrerequisite>RRY_YautjaTechBase</researchPrerequisite>
		</recipeMaker>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/Yautja_Light_Armour/Yautja_LightArmour</wornGraphicPath>
			<layers>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<tags>
				<li>YautjaLightChestplate</li>
				<li>YautjaLightArmour</li>
				<li>Yautja</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<thingCategories>
			<li>RRY_Armour</li>
		</thingCategories>
		<tradeTags>
			<li>Apparel</li>
		</tradeTags>
	</ThingDef>

